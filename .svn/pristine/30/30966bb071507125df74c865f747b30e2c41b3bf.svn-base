./tools/infrastructure/check_style.sh
echo &quot;===END of C++ code style========================&quot;</command>
    </hudson.tasks.Shell>
    <hudson.tasks.Shell>
      <command>#!/bin/bash
wget http://172.30.23.4:8081/artifactory/test/build_1.sh
chmod +x build_1.sh
echo &quot;Start build&quot;
./build_1.sh
curl -X PUT &quot;http://172.30.23.4:8081/artifactory/OpenSDL_${SDL_FOLDER}/${BUILD_TYPE}/${BUILD_NUMBER}/OpenSDL.tar.gz&quot; -T OpenSDL.tar.gz</command>
    </hudson.tasks.Shell>
